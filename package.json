{
  "name": "default-avatar-helper",
  "version": "2.0.140",
  "description": "A helper for creating default avatars.",
  "main": "dist/index.js",
  "module": "dist/default-avatar-helper.esm.js",
  "jsnext:main": "dist/default-avatar-helper.esm.js",
  "typings": "dist/index.d.ts",
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "run-s clean compile bundle",
    "bundle": "rollup -c",
    "check:format": "prettier --list-different \"**/*.{js,ts}\"",
    "check:types": "tsc --noEmit",
    "clean": "run-p clean:*",
    "clean:compiled": "shx rm -rf compiled",
    "clean:coverage": "shx rm -rf coverage",
    "clean:dist": "shx rm -rf dist",
    "compile": "tsc -p tsconfig.base.json",
    "format": "prettier --write \"**/*.{js,ts}\"",
    "lint": "eslint .",
    "postbundle": "shx rm -rf compiled && shx cp ./index.js ./dist/index.js",
    "release": "tanem-scripts release",
    "test": "run-s check:* lint build test:*",
    "test:cjs": "jest --config ./config/jest/config.cjs.js",
    "test:cjsprod": "jest --config ./config/jest/config.cjsprod.js",
    "test:esm": "jest --config ./config/jest/config.esm.js",
    "test:src": "jest --config ./config/jest/config.src.js",
    "test:umd": "jest --config ./config/jest/config.umd.js",
    "test:umdprod": "jest --config ./config/jest/config.umdprod.js"
  },
  "repository": "github:tanem/default-avatar-helper",
  "keywords": [
    "avatar",
    "avatar-generator",
    "avatar-placeholder",
    "avatars",
    "default-avatar",
    "helper",
    "javascript",
    "typescript"
  ],
  "author": "Tane Morgan (https://github.com/tanem)",
  "license": "MIT",
  "bugs": "https://github.com/tanem/default-avatar-helper/issues",
  "homepage": "https://github.com/tanem/default-avatar-helper",
  "dependencies": {
    "@babel/runtime": "^7.26.0",
    "lodash": "^4.17.21",
    "tslib": "^2.8.1"
  },
  "devDependencies": {
    "@babel/core": "7.26.0",
    "@babel/plugin-transform-runtime": "7.25.9",
    "@babel/preset-env": "7.26.0",
    "@eslint/js": "9.17.0",
    "@rollup/plugin-babel": "6.0.4",
    "@rollup/plugin-commonjs": "28.0.1",
    "@rollup/plugin-node-resolve": "15.3.0",
    "@rollup/plugin-replace": "6.0.1",
    "@rollup/plugin-terser": "0.4.4",
    "@types/eslint__js": "8.42.3",
    "@types/jest": "29.5.14",
    "@types/lodash": "4.17.13",
    "babel-plugin-lodash": "3.3.4",
    "eslint": "9.17.0",
    "globals": "15.11.0",
    "jest": "29.7.0",
    "npm-run-all2": "7.0.1",
    "prettier": "3.3.3",
    "rollup": "4.24.3",
    "shx": "0.3.4",
    "tanem-scripts": "7.0.27",
    "ts-jest": "29.2.5",
    "typescript": "5.6.3",
    "typescript-eslint": "8.12.2"
  },
  "engines": {
    "npm": ">=8.0.0"
  }
}
